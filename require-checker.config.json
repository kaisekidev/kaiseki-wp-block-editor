{
  "symbol-whitelist": [
    "null", "true", "false",
    "static", "self", "parent",
    "array", "string", "int", "float", "bool", "iterable", "callable", "void", "object",
    "add_action", "add_filter", "add_theme_support", "remove_theme_support",
    "wp_add_inline_script", "wp_dequeue_style",
    "wp_list_pluck",
    "WP_Post"
  ],
  "php-core-extensions": [
    "Core",
    "date",
    "pcre",
    "Phar",
    "Reflection",
    "SPL",
    "standard"
  ],
  "scan-files": []
}
